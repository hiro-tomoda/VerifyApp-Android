<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MenuFragment">

    <Button
        android:id="@+id/button_ui_parts"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="UI部品検証"/>

    <Button
        android:id="@+id/button_webapi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="WebAPI検証"/>

    <Button
        android:id="@+id/button_db"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="DB検証"/>

    <Button
        android:id="@+id/button_webview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="WebView検証"/>

    <Button
        android:id="@+id/button_calendar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="カレンダー検証"/>

    <Button
        android:id="@+id/button_bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ボトムナビゲーション検証"/>

    <Button
        android:id="@+id/button_preference"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="プリファレンス検証"/>

    <Button
        android:id="@+id/button_login"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="ログイン検証"/>

</LinearLayout>
